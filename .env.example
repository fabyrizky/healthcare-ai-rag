# Environment Variables for Healthcare AI & RAG Application
# Copy this file to .env and fill in your actual values

# Application Settings
APP_ENV=production
DEBUG_MODE=false
LOG_LEVEL=INFO

# API Keys (Optional - for enhanced features)
OPENAI_API_KEY=your_openai_api_key_here
QWEN_API_KEY=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

# News API Configuration (Optional)
NEWS_API_KEY=your_news_api_key_here
BING_SEARCH_API_KEY=your_bing_search_api_key_here

# Database Configuration (if using external database)
DATABASE_URL=postgresql://username:password@localhost:5432/healthcare_db
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=healthcare_db
DATABASE_USER=your_db_username
DATABASE_PASSWORD=your_db_password

# File Upload Settings
MAX_FILE_SIZE_MB=200
ALLOWED_FILE_TYPES=csv,xlsx,xls

# Security Settings
SECRET_KEY=your_secret_key_for_session_management
ENCRYPTION_KEY=your_encryption_key_for_sensitive_data

# External Service URLs
KNOWLEDGE_BASE_URL=https://your-knowledge-base-api.com
HEALTHCARE_API_URL=https://your-healthcare-api.com

# Cache Configuration
REDIS_URL=redis://localhost:6379/0
CACHE_TTL_SECONDS=3600

# Monitoring and Analytics
SENTRY_DSN=your_sentry_dsn_for_error_tracking
GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Email Configuration (for notifications)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# Compliance and Audit
AUDIT_LOG_ENABLED=true
HIPAA_COMPLIANCE_MODE=true
DATA_RETENTION_DAYS=365

# Performance Settings
MAX_CONCURRENT_USERS=100
RATE_LIMIT_PER_MINUTE=60
ENABLE_CACHING=true

# Feature Flags
ENABLE_AI_ASSISTANT=true
ENABLE_NEWS_FEEDS=true
ENABLE_SCENARIO_PLANNING=true
ENABLE_ADVANCED_ANALYTICS=true
